{
  "files.exclude": {
    "**/target": true,
    "**/Cargo.lock": true
  },
  "editor.semanticTokenColorCustomizations": {
    "rules": {
      "*.mutable": {
        "fontStyle": "" // set to empty string to disable underline, which is the default
      }
    }
  },
  "cSpell.words": [
    "ANNN",
    "BNNN",
    "BXNN",
    "COSMAC",
    "CXNN",
    "DXYN",
    "Spacefight"
  ]
}
